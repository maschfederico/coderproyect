n        console.log(\"Total Items Carrito:\")\r\n        console.log(cart.length)\r\n    }\r\n  \r\n    useEffect(() => {\r\n      setProducts(productos);\r\n      computarTotal();\r\n      computarItem();  \r\n      console.log(\"Productos:\");\r\n      console.log(productos);\r\n      console.log(\"carrito:\");\r\n      console.log(cart);\r\n    }, [cart]);\r\n  \r\n    return (\r\n      <Store.Provider\r\n        value={{\r\n          cart,\r\n          products,\r\n          totalCart,\r\n          itemCart,\r\n          addItemCart,\r\n          deleteFromCart\r\n        }}\r\n      >\r\n        {children}\r\n      </Store.Provider>\r\n    );\r\n  };\r\n  \r\n  export default StoreProvider;\r\n","C:\\Users\\masch\\OneDrive\\Documentos\\CoderHouse\\fibo-app\\src\\Components\\Cart\\CartWidget.jsx",[],"C:\\Users\\masch\\OneDrive\\Documentos\\CoderHouse\\fibo-app\\src\\Components\\general\\Nav\\Nav.jsx",[],"C:\\Users\\masch\\OneDrive\\Documentos\\CoderHouse\\fibo-app\\src\\Vistas\\Cart.jsx",[],"C:\\Users\\masch\\OneDrive\\Documentos\\CoderHouse\\fibo-app\\src\\Vistas\\DetalleProducto.jsx",["86","87"],"C:\\Users\\masch\\OneDrive\\Documentos\\CoderHouse\\fibo-app\\src\\Components\\ProductCard\\ProductCard.jsx",[],"C:\\Users\\masch\\OneDrive\\Documentos\\CoderHouse\\fibo-app\\src\\Components\\ProductCard\\ItemCount.jsx",["88"],"import {useEffect, useState, useContext} from 'react';\r\nimport  {Store} from '../../DataSource/cartcontext/store';\r\n\r\nconst Itemcount = ({cantidad_inicial,eventAddCarrito,stock, id}) => {\r\n    const [cantidad, setCantidad] = useState(0);\r\n\r\n    const store = useContext(Store);\r\n\r\n    const {addItemCart} = store;\r\n\r\n    const handleClickResta = () => {\r\n        if(cantidad> 1) {\r\n            setCantidad(cantidad - 1);\r\n        }\r\n    }\r\n\r\n    const handleClickSuma = () => {\r\n        if(cantidad < stock) {\r\n            setCantidad(cantidad + 1);\r\n        }\r\n    }\r\n\r\n    const agregarItemCarrito = ()=>{\r\n    addItemCart({productId: id, cantidad: cantidad});\r\n    eventAddCarrito(cantidad)\r\n    };\r\n\r\n    useEffect(()=>{\r\n        setCantidad(cantidad_inicial)\r\n    },[]);\r\n\r\n    return (  \r\n        <div className=\"container\">\r\n            <div className=\"row\">\r\n                <div className=\" col 4\">\r\n                    <button \r\n                        disabled={cantidad === 1 ? 'disabled' : null } \r\n                        onClick={handleClickResta}\r\n                    >\r\n                        -\r\n                    </button>\r\n                </div>\r\n                <div className=\" col 4\">\r\n                    {stock === \"0\"? <p> Sin stock</p> :<input type=\"text\" value={cantidad} readOnly/>\r\n                    }\r\n                    \r\n                </div>\r\n                <div className=\" col 4\">\r\n                    <button disabled = {cantidad >= stock || stock === \"0\" ? 'disabled': null} onClick={handleClickSuma}>+</button>\r\n                </div>\r\n            </div>   \r\n            <div className=\" row\">  \r\n                <button disabled = {stock === \"0\" ? 'disabled': null} onClick={agregarItemCarrito}>Agregar al carrito</button>\r\n            </div>\r\n        </div>\r\n    );\r\n    \r\n}\r\n \r\nexport default Itemcount;","C:\\Users\\masch\\OneDrive\\Documentos\\CoderHouse\\fibo-app\\src\\Vistas\\Home.jsx",[],"C:\\Users\\masch\\OneDrive\\Documentos\\CoderHouse\\fibo-app\\src\\Components\\general\\ItemListContainer\\ItemListContainer.jsx",[],"C:\\Users\\masch\\OneDrive\\Documentos\\CoderHouse\\fibo-app\\src\\Components\\general\\ItemListContainer\\ItemList.jsx",[],"C:\\Users\\masch\\OneDrive\\Documentos\\CoderHouse\\fibo-app\\src\\DataSource\\mockdata\\productos.js",[],"C:\\Users\\masch\\OneDrive\\Documentos\\CoderHouse\\fibo-app\\src\\DataSource\\firestore\\index.js",[],{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","replacedBy":"92"},{"ruleId":"89","replacedBy":"93"